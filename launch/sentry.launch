    <node pkg="main_control" type="sanitization_navigation_fsm.py" name="main_control"  required ="true" output="screen"/>
    <node pkg="main_control" type="counter.py" name="counter" output="screen"/>
    <node pkg="remote_communication" type="browser_reports.py" name="browser_reports" output="screen"/>
    <node pkg="remote_communication" type="heartbeat_jetson_remote.py" name="heartbeat" output="screen"/>
